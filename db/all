CREATE TABLE Requests (
RID INTEGER PRIMARY KEY AUTOINCREMENT,
FirstName TEXT NOT NULL,
LastName TEXT NOT NULL,
Email TEXT NOT NULL,
PhoneNumber TEXT,
CompanyName TEXT,
CompanyURL TEXT,
Timezone TEXT NOT NULL,
RDate TEXT NOT NULL,
Status INTEGER NOT NULL);

CREATE TABLE Users (
RID INTEGER NOT NULL,
Username TEXT NOT NULL,
Password TEXT NOT NULL,
UDate INTEGER NOT NULL,
Admin INTEGER NOT NULL,
FOREIGN KEY(RID) REFERENCES Requests(RID));

CREATE TABLE Access (
PID INTEGER NOT NULL,
RID INTEGER NOT NULL,
FOREIGN KEY(PID) REFERENCES Projects(PID),
FOREIGN KEY(RID) REFERENCES Requests(RID)
);

CREATE TABLE Projects (
PID INTEGER PRIMARY KEY AUTOINCREMENT,
PName VARCHAR NOT NULL,
PDescription VARCHAR NOT NULL,
AutoSave INTEGER NOT NULL
);

CREATE TABLE Classes (
CID INTEGER PRIMARY KEY AUTOINCREMENT,
CName VARCHAR NOT NULL,
CStaticID INTEGER NOT NULL,
PID INTEGER NOT NULL,
FOREIGN KEY(PID) REFERENCES Projects(PID)
);

CREATE TABLE Images (
IID INTEGER PRIMARY KEY AUTOINCREMENT,
IName VARCHAR NOT NULL,
PID INTEGER NOT NULL,
FOREIGN KEY(PID) REFERENCES Projects(PID)
);

CREATE TABLE Labels (
LID INTEGER PRIMARY KEY AUTOINCREMENT,
CID INTEGER NOT NULL,
X INTEGER NOT NULL,
Y INTEGER NOT NULL,
W INTEGER NOT NULL,
H INTEGER NOT NULL,
CStaticID INTEGER NOT NULL,
DateCreated INTEGER NOT NULL,
CreatedBy TEXT NOT NULL,
Type VARCHAR NOT NULL,
IID INTEGER NOT NULL,
FOREIGN KEY(CID) REFERENCES Classes(CID),
FOREIGN KEY(IID) REFERENCES Images(IID)
);

==

DROP TABLE Projects; DROP TABLE Classes; DROP TABLE Images; DROP TABLE Labels;

==
